datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

generator client {
  provider      = "prisma-client-js"
  binaryTargets = "native"
}

model TimeEntry {
  id          String    @id @default(cuid())
  title       String
  startTime   DateTime  @default(now())
  endTime     DateTime?
 }
